{
	"_version": "1.17.0",
	"sap.ui": {
		"_version": "1.3.0",
		"technology": "UI5",
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": ["sap_fiori3", "sap_belize"]
	},
	"sap.app": {
		"_version": "1.3.0",
		"id": "emc.fin.ar",
		"type": "application",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"tags": {
			"keywords": ["{{fruits}}"]
		},
		"ach": "FIN-ACC-AR"
	},
	"sap.ui5": {
		"_version": "1.2.0",
		"dependencies": {
			"minUI5Version": "1.56.10",
			"libs": {
				"sap.m": {
					"minVersion": "1.56.10"
				},
				"sap.ui.layout": {
					"minVersion": "1.56.10"
				}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"resources": {
			"css": [{
				"uri": "css/style.css",
				"id": ""
			}]
		},
		"rootView": {
			"id": "idAppView",
			"viewName": "emc.fin.ar.view.App",
			"type": "XML"
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewPath": "emc.fin.ar.view",
				"viewType": "XML",
				"controlId": "idAppCon"
			},
			"routes": [{
				"pattern": "",
				"name": "start",
				"target": ["master", "empty"]
			}, {
				"pattern": "myfruits/{fruitId}",
				"name": "detail",
				"target": ["master", "modi"]
			}, {
				"pattern": "supplierdata/{suppId}",
				"name": "supplier",
				"target": ["master", "biden"]
			}],
			"targets": {
				"master": {
					"viewName": "View1",
					"controlAggregation": "masterPages",
					"viewId": "idView1"
				},
				"empty": {
					"viewName": "Avengers",
					"controlAggregation": "detailPages",
					"viewId": "idEmpty"
				},
				"modi": {
					"viewName": "View2",
					"controlAggregation": "detailPages",
					"viewId": "idView2"
				},
				"biden": {
					"viewName": "Supplier",
					"controlAggregation": "detailPages",
					"viewId": "idSupplier"
				}
			}
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {},
				"uri": "i18n/i18n.properties",
				"preload": false,
				"async": true
			},
			"": {
				"type": "sap.ui.model.json.JSONModel",
				"uri": "model/fruitdata.json"
			}
		}
	}
}